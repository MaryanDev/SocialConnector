<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Content/Styles/site.css" rel="stylesheet" />
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Open+Sans'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/Styles/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Styles/upload-file.css" rel="stylesheet" />

    @RenderSection("HeadStyles", false)
    <style>
        html, body, h1, h2, h3, h4, h5 {
            font-family: "Open Sans", sans-serif
        }
    </style>

    @*<script src="~/Scripts/libs/jquery/jquery-3.2.1.js"></script>*@
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"
            integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="
            crossorigin="anonymous"></script>
    <script src="~/Scripts/libs/jquery/jquery-ajax-unobtrusive.js"></script>
    <script src="~/Scripts/libs/bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/libs/jquery/jquery-uploadfile.min.js"></script>

    @RenderSection("HeadScripts", false)

    <title>@ViewBag.Title</title>


    <script type="text/javascript">
    var Settings = {
        searchUrl: '@Url.Action("GetSiteSearchResult", "Search")',
        avatarUploadUrl: '@Url.Action("UploadAvatar", "Home")',
        profileHomeUrl: '@Url.Action("Profile", "Home", new { })'
    };
    </script>
</head>
<body class="w3-theme-l5">
    @{
        Html.RenderPartial("NavBar");
    }
    <div>
        @RenderBody()
    </div>

    @RenderSection("Footer", false)
    @RenderSection("FootScripts", false)
</body>
</html>
